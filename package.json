{
  "name": "kythira",
  "productName": "Kythira",
  "version": "1.0.0",
  "description": "Kubernetes visualizer",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "dev": "nodemon server/server.ts",
    "test": "cross-env NODE_ENV=test jest --verbose --coverage"
  },
  "keywords": [],
  "author": {
    "name": "Team Kythira",
    "email": "kythira@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "protocols": [
          {
            "name": "Koob App",
            "schemes": ["koob-app"]
          }
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "my_new_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "mimeType": ["x-schema-handler/koob-app"]
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "devContentSecurityPolicy": "default-src * 'unsafe-inline' 'unsafe-eval'",
            "renderer": {
              "nodeIntegration": true,
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            },
            "port": 3001
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.63",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/d3": "^7.4.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/pg": "^8.6.5",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "19.0.4",
    "eslint": "^8.17.0",
    "eslint-plugin-import": "^2.26.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.5.2",
    "jest": "^28.1.1",
    "jest-cli": "^28.1.1",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^28.1.1",
    "msw": "^0.42.1",
    "node-fetch": "^3.2.6",
    "node-loader": "^2.0.0",
    "nodemon": "^2.0.16",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typescript": "~4.5.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.16.3",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "d3": "^7.4.4",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.1",
    "pg": "^8.7.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0"
  }
}